{
  "name": "kubernetes",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx esbuild ./src/index.ts --bundle --platform=node --outdir=dist --format=cjs --loader:.ts=ts --sourcemap --minify --external:@prisma/client --external:express --external:jsonwebtoken --external:@aws-sdk/client-s3 --external:@aws-sdk/s3-request-presigner --external:bull --external:ioredis --external:ws --external:@google/genai --external:@anthropic-ai/sdk --external:razorpay --external:uuid --external:winston --external:zod",
    "start": "node dist/index.js",
    "dev": "yarn build && yarn start",
    "remove-logs": "npx ts-node src/scripts/remove-logs",
    "lint": "eslint './src/**/*.{ts,js}'",
    "format": "prettier --write './src/**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.4.0",
    "@winterfell/types": "*",
    "axios": "^1.13.2",
    "bullmq": "^5.63.0",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "zod": "^4.1.12"
  }
}
